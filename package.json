    "prettier-plugin-svelte": "^3.4.0",
    "sass-embedded": "^1.93.2",
    "svelte": "^5.43.2",
    "svelte-check": "^4.3.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
